{
    "globals": {
        "jQuery": true,
        "$": true
    },
    "env": {
        "browser": false,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "quotes": [1, "double"]
    }
}
