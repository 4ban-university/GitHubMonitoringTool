{
    "globals": {
        "jQuery": true,
        "$": true
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 1,
        "quotes": 1,
        "no-console": 0,
        "comma-dangle": 0,
        "no-unused-vars":1,
        "no-constant-condition":0
    }
}
